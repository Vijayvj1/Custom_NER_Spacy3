import pandas as pd

dict = {"loc" : (10,20)}

# Create a DataFrame from the dictionary
df = pd.DataFrame.from_dict(dict, orient='index', columns=['loc'])

# Split the tuple values of loc into separate columns
df[['loc_x', 'loc_y']] = pd.DataFrame(df.loc['loc'].tolist(), index=df.index)

# Drop the original 'loc' column
df.drop('loc', axis=0, inplace=True)

# Print the resulting DataFrame
print(df)
