def get_maximum_occurred(strings):
    count_dict = {}
    max_count = 0

    # Count occurrences of each string
    for string in strings:
        if string in count_dict:
            count_dict[string] += 1
        else:
            count_dict[string] = 1

        # Update the maximum count
        if count_dict[string] > max_count:
            max_count = count_dict[string]

    # Find the string(s) with the maximum count
    max_occurred_strings = []
    for string, count in count_dict.items():
        if count == max_count:
            max_occurred_strings.append(string)

    return max_occurred_strings

# Example usage
string_list = ["apple", "banana"]
max_occurred = get_maximum_occurred(string_list)
print("Max occurred string(s):", max_occurred)
